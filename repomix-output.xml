    # Guardar HR Enriched
    df_final = pd.DataFrame(records)
    df_final.to_csv(settings.HR_DATA_PATH, index=False)
    print(f"‚úÖ HR Data guardado en {settings.HR_DATA_PATH}")
    
    return records

def generate_demo_tickets():
